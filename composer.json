{
    "name": "carawebs/better-taxonomy-description",
    "description": "WordPress plugin for better taxonomy description.",
    "keywords": [
        "wordpress"
    ],
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "David Egan",
            "email": "david@carawebs.com",
            "homepage": "http://davidegan.me",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Carawebs/better-taxonomy-description/issues",
        "source": "https://github.com/Carawebs/better-taxonomy-description"
    },
    "license": "MIT",
    "require": {
        "php": ">=5.4"
    },
    "autoload": {
        "psr-4": {
            "BetterTaxonomyDescription\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        }
    }
}
